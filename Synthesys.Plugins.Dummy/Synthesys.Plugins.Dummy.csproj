<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <RazorLanguage>3.0</RazorLanguage>
    <Authors>Anthony Turner</Authors>
    <Product>Dummy Developer Plugin for Synthesys</Product>
    <Description>Demonstrates how a plugin can provide Action and Reaction extensions</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
