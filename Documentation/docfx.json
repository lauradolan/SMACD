{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/**.csproj" ],
          "exclude": [ "Compass/**", "Synthesys/**" ],
          "src": "D:/SMACD"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "*.png",
          "favicon.ico",
          "favicon-*.png",
          "mstile-*.png",
          "safari-pinned-tab.svg",
          "browserconfig.xml",
          "site.webmanifest",
          "images/**",
          "images/favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "SMACD.io",
      "_appFooter": "&copy; 2019 Anthony Turner<br />This website and the associated libraries are MIT-licensed and may be redistributed under those terms.",
      "_appLogoPath": "images/Logo48.png",
      "_appFaviconPath": "favicon.ico"
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}